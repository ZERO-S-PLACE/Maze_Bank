<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<VBox maxHeight="-Infinity"
      maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="750.0"
      prefWidth="180.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.zeros.maze_bank_final_project.Controllers.Admin.AdminMenuController">
    <AnchorPane prefHeight="743.0" prefWidth="180.0" styleClass="left_container"
                stylesheets="@../../Styles/clientMenu.css">
        <VBox layoutX="40.0" layoutY="14.0" prefHeight="80.0" prefWidth="160.0" styleClass="title_container"
              stylesheets="@../../Styles/clientMenu.css" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="0.0">
            <FontAwesomeIconView glyphName="BANK" size="30"/>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title_container" text="Maze Bank"/>
        </VBox>
        <VBox layoutX="14.0" layoutY="80.0" prefHeight="400.0" prefWidth="160.0" styleClass="menu_container"
              stylesheets="@../../Styles/clientMenu.css" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="100.0">
            <Button fx:id="createClientBtn" mnemonicParsing="false" text="Create Client">
                <graphic>
                    <FontAwesomeIconView glyphName="PLUS" size="20"/>
                </graphic>
            </Button>
            <Button fx:id="clientsJuxtapositionBtn" mnemonicParsing="false" text="Client">
                <graphic>
                    <FontAwesomeIconView glyphName="LIST" size="20"/>
                </graphic>
            </Button>
            <Button fx:id="depositBtn" mnemonicParsing="false" text="Deposit">
                <graphic>
                    <FontAwesomeIconView glyphName="MONEY" size="20"/>
                </graphic>
            </Button>
            <Button fx:id="logoutBtn" mnemonicParsing="false" styleClass="alt_menu_btn" text="Logout">
                <graphic>
                    <FontAwesomeIconView glyphName="EXTERNAL_LINK" size="20"/>
                </graphic>
            </Button>
        </VBox>
    </AnchorPane>
</VBox>
